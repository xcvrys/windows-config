{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "root <#ffffff>in</> ",
          "type": "root"
        },
        {
          "foreground": "#E0DEF4",
          "properties": {
            "style": "folder",
            "home_icon": "\ueb06"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#191724",
          "background": "#E0DEF4",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#EBBCBA{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "branch_icon": "\ue725"
          },
          "style": "plain",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#31748F",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#EB6F92",
          "style": "plain",
          "template": "ERROR ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#E0DEF4",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#E0DEF4",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#E0DEF4",
          "style": "diamond",
          "template": " {{ round .PhysicalPercentUsed .Precision }}%",
          "type": "sysinfo"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E0DEF4",
          "style": "plain",
          "template": "~ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}}",
  "version": 2
}
